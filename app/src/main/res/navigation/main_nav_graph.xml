<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav_graph"
    app:startDestination="@id/venue_list">

    <fragment
        android:id="@+id/venue_list"
        android:name="com.example.venueassignment.ui.fragment.searchfragment.SearchFragment"
        android:label="venues"
        tools:layout="@layout/fragment_venue_search">
        <action
            android:id="@+id/open_detail_fragment"
            app:destination="@id/detail_fragment" />
    </fragment>

    <fragment
        android:id="@+id/detail_fragment"
        android:name="com.example.venueassignment.ui.fragment.detailfragment.DetailFragment"
        android:label="detail"
        tools:layout="@layout/fragment_venue_detail">
        <argument
            android:name="venueParcelData"
            app:argType="com.example.venueassignment.networkinterface.remote.response.search.Venues" />

    </fragment>


</navigation>
